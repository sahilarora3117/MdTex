{
  "name": "mdtex",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^4.6.0",
    "chokidar": "^3.5.1",
    "cross-env": "^7.0.3",
    "css-to-object": "^1.1.0",
    "custom-electron-titlebar": "^3.2.6",
    "electron-context-menu": "^2.5.0",
    "electron-editor-context-menu": "^1.1.1",
    "electron-is-dev": "^1.2.0",
    "electronmon": "^2.0.1",
    "html2canvas": "*",
    "jspdf": "^2.3.1",
    "markdown-to-jsx": "^7.1.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^7.2.1",
    "react-custom-electron-titlebar": "^3.1.0",
    "react-dom": "^17.0.1",
    "react-file-reader": "^1.1.4",
    "react-latex": "^2.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-split-pane": "^0.1.92",
    "react-to-print": "^2.12.3",
    "styled-components": "^5.2.1",
    "wait-on": "^5.2.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.6.0"
  },
  "build": {
    "appId": "com.github.mdtex",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "public/start-electron.js",
  "scripts": {
    "react-start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently -k \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron-package": "./node_modules/.bin/electron-builder -c.extraMetadata.main=build/start-electron.js",
    "preelectron-package": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
